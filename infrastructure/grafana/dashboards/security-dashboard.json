        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "text": {},
        "textMode": "auto"
      },
      "pluginVersion": "8.1.2",
      "targets": [
        {
          "expr": "clamp_min(2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Auth.*\", severity=\"critical\"}) > 0) * 2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Auth.*\", severity=\"warning\"}) > 0), 0)",
          "interval": "",
          "legendFormat": "Authentication",
          "refId": "A"
        },
        {
          "expr": "clamp_min(2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Token.*\", severity=\"critical\"}) > 0) * 2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Token.*\", severity=\"warning\"}) > 0), 0)",
          "interval": "",
          "legendFormat": "Token Management",
          "refId": "B"
        },
        {
          "expr": "clamp_min(2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Credential.*\", severity=\"critical\"}) > 0) * 2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Credential.*\", severity=\"warning\"}) > 0), 0)",
          "interval": "",
          "legendFormat": "Credential Management",
          "refId": "C"
        },
        {
          "expr": "clamp_min(2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Conjur.*\", severity=\"critical\"}) > 0) * 2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Conjur.*\", severity=\"warning\"}) > 0), 0)",
          "interval": "",
          "legendFormat": "Conjur Vault",
          "refId": "D"
        },
        {
          "expr": "clamp_min(2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Unauthorized.*\", severity=\"critical\"}) > 0) * 2 - (sum(ALERTS{alertstate=\"firing\", alertname=~\".*Unauthorized.*\", severity=\"warning\"}) > 0), 0)",
